<% if @current_user %>

<div id="home-links">
  <div class="link"><%= link_to show_svg('subscriptions.svg'), user_subscriptions_path(@current_user) %></div>
  <div class="link"><%= link_to show_svg('inspirations.svg'), user_inspirations_path(@current_user) %></div>
  <div class="link"><%= link_to show_svg('priorities.svg'), user_priority_items_path(@current_user) %></div>
  <div class="link"><%= link_to show_svg('discipline.svg'), user_disciplines_path(@current_user) %></div>
  <div class="link"><%= link_to show_svg('quests.svg'), user_objectives_path(@current_user) %></div>
  <div class="link"><%= link_to show_svg('family_members.svg'), user_family_members_path(@current_user) %></div>
</div>

<% else %>

<div id="home-links">
  <div class="link"><%= link_to show_svg('sign_up.svg'), signup_path %></div>
  <div class="link"><%= link_to show_svg('sign_in.svg'), login_path %></div>
</div>

<% end %>
