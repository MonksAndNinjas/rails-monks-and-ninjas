<%= form_for object do |f| %>

  <%= f.fields_for :disciplines do |dis| %>

    <% if dis.object.title.blank? %>
      <%= dis.label :title %><br>
      <%= dis.text_field :title %><br>

      <%= dis.label :content %><br>
      <%= dis.text_field :content %><br>

      <%= dis.label :duration %><br>
      <%= dis.text_field :duration %><br>
    <% end %>

  <% end %>

  <br></br>

  <%= f.password_field :password %>

  <%= f.submit "Add Discipline"%>
<% end %>
