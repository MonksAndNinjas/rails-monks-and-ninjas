<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <tr>

      <td><strong>Title</strong></td>
      <td><strong>Content</strong></td>
      <td><strong>Duration</strong></td>

    </tr>
    <% all_of_attribute.each do |discipline| %>
      <tr>

        <td><%= discipline.title %></td>
        <td><%= discipline.content %></td>
        <td><%= discipline.duration %></td>
        <td><%= link_to "X", user_discipline_path(@current_user, discipline), method: :delete, data: { confirm: "Really?" } %></td>

      </tr>
    <% end %>
  </tbody>
</table>
