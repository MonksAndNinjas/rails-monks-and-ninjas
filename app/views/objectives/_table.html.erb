<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <tr>

      <td><strong>Type</strong></td>
      <td><strong>Title</strong></td>
      <td><strong>Description</strong></td>

    </tr>
    <% @quests.each do |quest| %>
      <tr>

        <td><%= quest_duration(quest) %> Quest</td>
        <td><%= quest.title %></td>
        <td><%= quest.description %></td>
        <td><%= link_to "X", user_objective_quest_path(@current_user, quest.objective_id, quest.id), method: :delete, data: { confirm: "Really?" } %>
          /<%= link_to "E", edit_user_objective_quest_path(@current_user, quest.objective_id, quest.id) %></td>
        </td>

      </tr>
    <% end %>
  </tbody>
</table>
