<div class="container">
  <%= show_svg('content_section.svg') %>

  <div class="title">
    <h1 class="attr-title"></h1>
  </div>

  <article></article>

  <section></section>

  <div class="actions"></div>
</div>

<div class="loader"></div>

<script type="text/javascript" charset="utf-8">
$(function () {
  $(window).load(function() {
     $('.loader').hide();
  });

  current_user = new User;

  $('section').html('What to do, What to do!?<br>Click on a link to the left');

  $(".attr").on("click", function() {
    getUserData(this);
  });
});
</script>
