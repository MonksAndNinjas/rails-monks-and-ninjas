<div id="logo">
  <%= link_to show_svg('lifetitle.svg'), root_url %>
</div>

<% if @current_user %>

<div class="dropdown">
  <%= show_svg('profile.svg') %>
  <div class="dropdown-content">
    <a href="/users/<%= @current_user.id %>">Profile Page</a>
    <a class="my_life" href="/my_life">My Life</a>
    <a href="/my_life/display_quests">My Quests</a>
    <%= link_to "Log Out", logout_path, method: :delete %>
  </div>
</div>

<% end %>
