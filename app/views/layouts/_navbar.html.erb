<!-- always display home and set proper font -->
<%= display_home_link %>

<% if @current_user %>
  <%= display_user_link %>

    <%= link_to "Log Out", logout_path, method: :delete %>

  <br></br>

  <% nested_attributes_keys.each do |attr| %>
    <%= display_nested_attribute_link(attr) %>
  <% end %>
<% else %>
<!-- display links to sign up or log in if not logged in -->
  <%= display_sign_up %>

  <%= display_log_in %>
<% end %>
