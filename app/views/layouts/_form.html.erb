<%= form_for object do |f| %>

  <%= f.fields_for :attributes do |attr| %>
    <% @attribute.attributes_keys.each do |key| %>

      <%= attr.label "#{key.to_s}" unless @attribute.create_label?(key) %><br>

      <% a = @attribute.create_field_for(key) %>
      <%= create_field(key, a, attr, @association) %><br>

    <% end %>
  <% end %>

  <br></br>

  <%= f.password_field :password %>

  <%= f.submit "Create"%>
<% end %>
