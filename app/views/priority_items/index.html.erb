<h1>Your Priorities</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @priority_items.each do |item| %>
      <tr>

        <td><%= item.title %></td>
        <td><%= item.description %></td>
        <td><%= link_to "X", subscription, method: :delete, data: { confirm: "Really?" } %></td>

      </tr>
    <% end %>
  </tbody>
</table>
